        <!-- ======= Hero Section ======= -->
            <section id="hero" class="d-flex align-items-center">
                <div id="home"  class="container position-relative" data-aos="fade-up" data-aos-delay="500">
                <h1 >Welcome <%= title %></h1>
                <h2></h2>
                <!-- <a href="#about" class="btn-get-started scrollto">About Us</a> -->
                </div>
            </section><!-- End Hero -->

            <section id="controls" class="d-flex justify-content-center">
              <div class="container controls">
                <div class="row">
                  <div class="col-6 d-flex justify-content-center">
                    <button type="button" class="btn btn-outline-light">Modify</button>
                  </div>
                  <div class="col-6 d-flex justify-content-center">
                    <button type="button" class="btn btn-outline-light">Delete</button>
                  </div>
                </div>
              </div>
            </section>

            <! -- ======== Data Section ========= -->

            <div class="container">
              <div class="row ">

                  <!--/row-->
                  <!-- Our Clients -->
                  <div class="col-md-9">
                      <!--Form Portlet-->
                      <div class="donation-form-outer">
                          <div class="form-portlet">
                              <h3>Login Information</h3>

                              <div class="row clearfix">

                                  <div class="form-group col-lg-6 col-md-6 col-sm-6 ">
                                      <div class="field-label">Full Name <span class="required">*</span></div>
                                      <input name="ctl00$ContentPlaceHolder1$txtName" type="text" id="ctl00_ContentPlaceHolder1_txtName" required="" placeholder="Enter Name" />
                                  </div>

                                  <div class="form-group col-lg-6 col-md-6  col-sm-6 ">
                                      <div class="field-label">
                                          Email <span class="required">*</span>
                                      </div>
                                      <input name="ctl00$ContentPlaceHolder1$txtEmail" type="text" id="ctl00_ContentPlaceHolder1_txtEmail" placeholder="Email Id" />
                                  </div>

                                  <div class="form-group col-lg-6 col-md-6  col-sm-6 ">
                                      <div class="field-label">
                                          Mobile No. <span class="required">*</span>
                                          <span id="ctl00_ContentPlaceHolder1_rvDigits" style="color:Red;visibility:hidden;">Valid No</span>
                                      </div>
                                      <input name="ctl00$ContentPlaceHolder1$txtMobile" type="text" id="ctl00_ContentPlaceHolder1_txtMobile" required="" placeholder="Mobile No." />
                                  </div>
                              </div>

                            
    
                              <h3>Contact Information</h3>
                              <div class="row ">


                                <div class="form-group col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                  <div class="field-label">Address</div>
                                  <textarea name="ctl00$ContentPlaceHolder1$txtAddress" rows="3" cols="20" id="ctl00_ContentPlaceHolder1_txtAddress" placeholder="Enter City or Village"></textarea>

                                </div>

                                  <div class="form-group col-lg-4 col-md-4 col-sm-6 col-xs-12">
                                      <div class="field-label">
                                          Pin/Zip Code 

                                          <span id="ctl00_ContentPlaceHolder1_RegularExpressionValidator3" style="color:Red;visibility:hidden;">No Valid</span>
                                      </div>

                                      <input name="ctl00$ContentPlaceHolder1$txtPinCode" type="text" id="ctl00_ContentPlaceHolder1_txtPinCode" placeholder="Pin Code" />
                                  </div>



                                  <div class="form-group col-lg-4 col-md-4  col-sm-4 col-xs-12">
                                      <label class="field-label">
                                          State</label>
                                      <select name="ctl00$ContentPlaceHolder1$cboState" id="ctl00_ContentPlaceHolder1_cboState" class="form-control form-group-margin ">
                                        <option value=""></option>

                                      </select>
                                      <input type="hidden" name="ctl00$ContentPlaceHolder1$ccdState_ClientState" id="ctl00_ContentPlaceHolder1_ccdState_ClientState" />
                                  </div>
                                  <div class="form-group col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                      <label class="field-label">
                                          Dist</label>
                                      <select name="ctl00$ContentPlaceHolder1$cboCity" id="ctl00_ContentPlaceHolder1_cboCity" class="form-control form-group-margin ">
                                        <option value=""></option>

                                      </select>
                                      <input type="hidden" name="ctl00$ContentPlaceHolder1$ccdCity_ClientState" id="ctl00_ContentPlaceHolder1_ccdCity_ClientState" />
                                  </div>
                                  <div class="form-group col-lg-4 col-md-4  col-sm-4 col-xs-12">
                                      <label class="field-label">
                                          Tehasil</label>
                                      <select name="ctl00$ContentPlaceHolder1$cboTeh" id="ctl00_ContentPlaceHolder1_cboTeh" class="form-control form-group-margin">
                                        <option value=""></option>

                                      </select>
                                      <input type="hidden" name="ctl00$ContentPlaceHolder1$ccdTehasil_ClientState" id="ctl00_ContentPlaceHolder1_ccdTehasil_ClientState" />
                                    </div>

                                  <div class="form-group col-lg-12 col-md-12 ">
                                      <input type="submit" name="ctl00$ContentPlaceHolder1$btnSave" value="Submit" onclick="" id="ctl00_ContentPlaceHolder1_btnSave" class="theme-btn btn-style-one  pull-right" />
                                  </div>


                              </div>
                              <h3>Find Blood</h3>
                              

                              <div class="row clearfix">
                                  <div class="form-group col-lg-3 col-md-6 col-sm-6 col-xs-12">
                                      <div class="field-label">Blood Group <span class="required">*</span></div>
                                      <select name="ctl00$ContentPlaceHolder1$cboBlood" id="ctl00_ContentPlaceHolder1_cboBlood" class="custom-select-box">
                                        <option value="00000000-0000-0000-0000-000000000000">--Blood Group--</option>
                                        <option value="06000000-0000-0000-0000-000000000002">A-</option>
                                        <option value="06000000-0000-0000-0000-000000000001">A+</option>
                                        <option value="06000000-0000-0000-0000-000000000006">AB-</option>
                                        <option value="06000000-0000-0000-0000-000000000005">AB+</option>
                                        <option value="06000000-0000-0000-0000-000000000004">B-</option>
                                        <option value="06000000-0000-0000-0000-000000000003">B+</option>
                                        <option value="06000000-0000-0000-0000-000000000008">O-</option>
                                        <option value="06000000-0000-0000-0000-000000000007">O+</option>
                                    
                                      </select>
                                  </div>



                                  <div class="form-group col-lg-3 col-md-6 col-sm-6 col-xs-12 ">
                                      <label class="field-label">
                                          State</label>
                                      <select name="ctl00$ContentPlaceHolder1$cboState" id="ctl00_ContentPlaceHolder1_cboState" class="form-control form-group-margin ">
                                        <option value=""></option>
                                    
                                      </select>
                                      <input type="hidden" name="ctl00$ContentPlaceHolder1$ccdState_ClientState" id="ctl00_ContentPlaceHolder1_ccdState_ClientState" />
                                  </div>
                                  <div class="form-group col-lg-3 col-md-6 col-sm-6 col-xs-12">
                                      <label class="field-label">
                                          Dist</label>
                                      <select name="ctl00$ContentPlaceHolder1$cboCity" id="ctl00_ContentPlaceHolder1_cboCity" class="form-control form-group-margin ">
                                        <option value=""></option>
                                    
                                      </select>
                                      <input type="hidden" name="ctl00$ContentPlaceHolder1$ccdCity_ClientState" id="ctl00_ContentPlaceHolder1_ccdCity_ClientState" />
                                  </div>
                                  <div class="form-group col-lg-3 col-md-6 col-sm-6 col-xs-12 ">
                                      <label class="field-label">
                                          Tehasil</label>
                                      <select name="ctl00$ContentPlaceHolder1$cboTeh" id="ctl00_ContentPlaceHolder1_cboTeh" class="form-control form-group-margin">
                                        <option value=""></option>
                                    
                                      </select>
                                      <input type="hidden" name="ctl00$ContentPlaceHolder1$ccdTehasil_ClientState" id="ctl00_ContentPlaceHolder1_ccdTehasil_ClientState" />
                                  </div>



                                  <div class="form-group col-lg-12 col-md-12 col-xs-12">
                                      <span id="ctl00_ContentPlaceHolder1_lblError"></span>
                                      <input type="submit" name="ctl00$ContentPlaceHolder1$btnSave" value="Search" id="ctl00_ContentPlaceHolder1_btnSave" class="theme-btn btn-style-one  pull-right" />
                                  </div>
                              </div>
                          </div>
                    </div>
                </div>
              </div>
